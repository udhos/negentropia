# texture_cube.obj

mtllib texture_cube.mtl

o cube

# square bottom
v -1 -1 -1
v -1 -1 1
v 1 -1 1
v 1 -1 -1

# square top
v -1 1 -1
v -1 1 1
v 1 1 1
v 1 1 -1

# uv coord

# red -3
vt 0 0

# green -2
vt .5 0

# blue -1
vt 1 0

usemtl 3-pixel-rgb

# face down (green -2)
f -6/-2 -7/-2 -8/-2
f -8/-2 -5/-2 -6/-2

# face up (green -2)
f -1/-2 -4/-2 -3/-2
f -3/-2 -2/-2 -1/-2 

# face right (red -3)
f -5/-3 -1/-3 -2/-3
f -2/-3 -6/-3 -5/-3

# face left (red -3)
f -7/-3 -3/-3 -4/-3
f -4/-3 -8/-3 -7/-3

# face front (blue -1)
f -6/-1 -2/-1 -3/-1
f -3/-1 -7/-1 -6/-1

# face back (blue -1)
f -8/-1 -4/-1 -1/-1
f -1/-1 -5/-1 -8/-1
